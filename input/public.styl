@import "reset.css";
.w{max-width: 1340px;width: 100%;margin: 0 auto;}
body{
	font-size: 100%;
}

.hcl-layout
	position relative
	width 1092px
	height 534px
	overflow scroll
.wrap
	position relative
	width 1600px
	table
		width 100%
	table
		thead
			background #E1E1E1
		tr
		th
		td
			font-weight normal
			font-size 14px
			color #000
			text-align center
			line-height 30px
			border 1px solid #c8c8c8
		tbody
			tr
			td
				color #7a7991
		span
			display block
		.red
			color red
		.bgF8F8F8
			background #F8F8F8
			&.hover
				background #F2DDDD
			&.focus
				background #F2B2B2
		.bgF2F2F2
			background #F2F2F2
			&.hover
				background #F2DDDD
			&.focus
				background #F2B2B2
		.bg-blue
			background #DEF1FE
			&.hover
				background #DEDCE9
			&.focus
				background #DEB1BE
			&.active
				color #fff
				background #007092
		.bg-yellow
			background #FFF9DE
			&.hover
				background #FFE4C9
			&.focus
				background #FFB99E
			&.active
				color #fff
				background #00A14C
		.analog-four
		.analog-six
		.analog-eight
			&.active
				color #fff
				background #007092
		.analog-three
		.analog-five
		.analog-seven
			&.active
				color #fff
				background #00A14C
		.area-two
		.area-nine
			&.round
				color #7a7991
				span
					position relative
					display block
					width 80%
					height 80%
					margin 0 auto
					border-radius 50%
					background #0066CC
					color #fff
					z-index 10
		.area-nine
			&.round
				span
					background #B90400
		.analog-two
		.analog-nine
			&.active
				color #7a7991
				span
					position relative
					display block
					width 80%
					height 80%
					margin 0 auto
					border-radius 50%
					background #0066CC
					color #fff
					z-index 10
		.analog-nine
			&.active
				span
					background #B90400
		.bg-yellow-light
			background #FDFCD8


#canvas
	position absolute
	left 0
	top 0
	pointer-events none
